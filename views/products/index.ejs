    <%-include("../partials/header"); %>
    <h1>My All Products</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Product Name</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i=0;i<products.length;i++){%>
                <tr><td><%=products[i].name %></td>
                    <td> <a class="btn btn-warning btn-sm mr-2" href="/products/edit/<%=products[i]._id%>">Edit</a>
                        <a class="btn btn-danger btn-sm mr-2" href="/products/delete/<%=products[i]._id%>">Delete</a>
                    </td> 
                </tr>
            <% }%>
        </tbody>
    </table>
    <%-include("../partials/footer"); %>